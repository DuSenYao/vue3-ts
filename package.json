    "dependency-cruiser": "^12.10.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.4.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
