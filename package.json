    "vuex": "^4.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.13",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
